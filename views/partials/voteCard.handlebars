<div class="post"  size="{{size}}" type="{{type}}">
				<div class="mediabox">
                    <img src="{{photoURL}}" class="imgBox">
                    <div class="textBox">{{description}}</div>
                </div>
				<div class="resultbox "><canvas id="{{chartid}}"></canvas></div>
				<div class="infobox"><fieldset class="votebox" name="{{chartid}}">
                    <legend></legend>
					{{#each options}}
                    {{> options}}
                    {{/each}}

				</fieldset>
				</div>
				<div class="buttonbar">
				<button class="submitbutton">submit</button>
				<button class="showbutton">Show</button>
				<button class="hidebutton">Hide</button>
                </div>
			</div>